{
    "name": "league/squery",
    "description": "PHP wrapper for OSQuery.",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Pitt",
            "email": "cgpitt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/process": "2.*",
        "aura/sqlquery": "2.*"
    },
    "autoload": {
        "psr-4": {
            "League\\Squery\\": "source"
        }
    },
    "require-dev": {
        "php": ">=5.5.0",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*"
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "League\\Squery\\Tests\\": "tests"
        }
    }
}
